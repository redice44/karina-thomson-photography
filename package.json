  "main": "index.js",
  "scripts": {
    "build": "tsc && mkdir -p ./dist/server/views && cp ./src/server/views/* ./dist/server/views",
    "clean": "find ./src -type f -name '*.js' -delete && find ./src -type f -name '*.js.map' -delete",
    "clean-dist": "rm -rf ./dist",
    "start:dev": "nodemon --watch dist --delay 2 -e js dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch src -e ts --exec \"npm run build\""
  },
